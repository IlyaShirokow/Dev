@model IList<WebApplication1.Models.Domain.Test>

@{
}

<h1>Test</h1>
<a type="submit" class="btn btn-primary" asp-area="" asp-controller="Test" asp-action="Add">Submit</a>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Surname</th>
            <th>Email</th>
            <th>Age</th>
            <th>Data Of Birth</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var test in Model)
        {
            <tr>
                <td>@test.Id</td>
                <td>@test.Name</td>
                <td>@test.Surname</td>
                <td>@test.Email</td>
                <td>@test.Age</td>
                <td>@test.DateOfBirtch.ToString("dd-MM-yyyy")</td>
                <td><a href="Test/View/@test.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>